{
  "name": "@deuscx/utils",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@7.32.3",
  "author": "Deuscx",
  "license": "MIT",
  "funding": "https://github.com/sponsors/deuscx",
  "homepage": "https://github.com/deuscx/@deuscx/utils#readme",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deuscx/@deuscx/utils.git"
  },
  "bugs": "https://github.com/deuscx/@deuscx/utils/issues",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "tsx ./src/index.ts",
    "lint": "eslint .",
    "lint:fix": "nr lint --fix",
    "release": "bumpp && npm publish",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:update": "vitest -u",
    "prepublishOnly": "nr build"
  },
  "devDependencies": {
    "@deuscx/eslint-config": "^0.0.8",
    "bumpp": "^9.1.0",
    "eslint": "^8.40.0",
    "lint-staged": "^13.2.2",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "vite": "4.3.5",
    "vite-plugin-inspect": "^0.7.25",
    "vitest": "^0.31.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ]
  }
}
